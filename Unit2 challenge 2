class player:
     def play(slef):
         print("the player is playing cricket.")
class Batsman (player):
     def play(self):
        print("the batsman is batting .")
class Bowler:
     def play(self):
         print("the bowler is bowling.")
# creating objects of the Batsman and bowler classes
batsman=Batsman()
bowler=Bowler()
# calling  the play() method for each object 
batsman.play()
bowler.play()
